{
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
    "rootDir": "./src/client",
    "outDir": "dist/client",
    "strict": true,
    "esModuleInterop": true,
    "useUnknownInCatchVariables": false,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "strictPropertyInitialization": false
  },
  "include": ["src/client/**/*","src/assets/**/*"],
  "exclude": ["src/server", "node_modules"]
}